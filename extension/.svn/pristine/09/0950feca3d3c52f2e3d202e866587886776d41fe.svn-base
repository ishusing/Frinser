<h1>Bootstrap Modal Steps jQuery Plugin Demos</h1>

<!-- Button trigger modal -->
<button type="button" class="btn btn-primary btn-lg" data-toggle="modal" data-target="#myModal"> Launch demo modal </button>

<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog" style="width: 441px;">
    <div class="modal-content" >
      <div class="modal-header">
        <h4 class="js-title-step"></h4>
      </div>
      <div class="modal-body">
        <div class="row hide" data-step="1" data-title="Connect Facebook:" style="
    background-color: #3c5a98;
    height: 87.4px;
    width: 441px;
">
          <div ><iframe src="https://softctrl.in/public/fbButton__.html" align="middle" style="width: 100%; height: 81px; border: none;"></iframe></div>
        </div>
        <div class="row hide" data-step="2" data-title="Connect Google:" style="
    background-color: #DD4D3B;
    height: 87.4px;
    width: 441px;
">
    <div ><iframe src="https://softctrl.in/public/gapi__.html" align="middle" style="width: 100%; height: 86px; border: none;"></iframe></div>
        </div>
  <div class="row hide" data-step="3" data-title="Connect Twitter:" style="
    background-color: #55ACEE;
    height: 87.4px;
    width: 440px;
">
    <div ><iframe src="https://softctrl.in/twitteroauth-master/indexButton__.php" align="middle" style="width: 100%; height: 81px; border: none;"></iframe></div>
        </div>

      </div>
      <div class="modal-footer">
      	<div id="status"></div>
        <button type="button" class="btn btn-warning js-btn-step" data-orientation="previous"></button>
        <button type="button" class="btn btn-success js-btn-step" data-orientation="next"></button>
      </div>
    </div>
  </div>
</div>
<script>
$('#myModal').modalSteps();
</script>
