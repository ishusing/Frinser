            .google_container {
                width:500px;
                abackground-color:red;
                height:400px;
            }
            .frinser_container {
                width:100%;
                max-height:100px;
/*              min-height:40px; */
                abackground-color:black;
            }
            .social_container {
                float:left;
                width:calc(40% - 10px); 
            }
            .custom_container {
                float:left;
                width:calc(40% - 10px);
                margin-left:15px;
            }
            .details_container {
            /*    float:left;	*/
                width:calc(100% - 20px);
                border:1px #AAA; /* dashed; */
                margin:5px;
            /*    margin-top:10px;	*/
            	padding-top:5px;
                margin-left:0px;
                font-family:calibri;
                overflow:hidden;
                text-align:center;
            }
            .details_container div {
                float:left;
                width:calc(100% / 6 - 6px);
                padding:3px;
                color:white;
                font-size:13px;
            }
            .details_container div:nth-child(1) {
                background-color:#3b5998; 
            }
            .details_container div:nth-child(2) {
                background-color:#d34836;
            }
            .details_container div:nth-child(3) {
                background-color:#1dcaff;
            }
            .details_container div:nth-child(4) {
                background-color:green;
            }
            .details_container div:nth-child(5) {
                background-color:black;
            }
            .details_container div:nth-child(6) {
                background-color:#CCC;
            }
            .disabled {
                opacity:0.6;
            }
            .progress_bar {
                width:100%;
                height:5px;
                background-color:#EEE;
                overflow:hidden;
                box-shadow:0px 0px 1px #EEE;
                cursor: pointer;
            }
            .progress_bar > .progress {
                float:left;
                height:100%;
                background-color:#999;
                max-width:100%;
                width:50%;
            }
            .pb_social {
                box-shadow:0px 0px 1px #EEE;
            }

            .pb_social > .progress:nth-child(1) {
                background-color:#3b5998;
                width:4%;
            }
            .pb_social > .progress:nth-child(2) {
                background-color:#d34836;
                width:4%;
            }
            .pb_social > .progress:nth-child(3) {
                background-color:#1dcaff;
                width:4%;
            }
            .pb_custom {
                background-color:#DDD;
            }
            .pb_custom > .progress {
                border-top-right-radius:5px;
                background-color:green;
            }
            .circle {
				float:left;
				margin-left: 15px;
				width:8px;
				height:8px;
				border-radius:4px;
				font-size:2px;
				color:#fff;
				line-height:8px;
				text-align:center;
				background:green;
				cursor: pointer;
			}
